{
    "name": "leo-yi/laravel-fillable",
    "description": "A Laravel package that generates php array keys from table fields",
    "keywords": [
        "laravel",
        "laravel-fillable"
    ],
    "homepage": "https://github.com/leo-yi/laravel-fillable",
    "license": "MIT",
    "authors": [
        {
            "name": "leo",
            "email": "uyihoo@gmail.com",
            "role": "owner"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=7.0",
        "doctrine/dbal": "^3.2",
        "spatie/laravel-package-tools": "^1.9.2",
        "composer-runtime-api": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Leoyi\\LaravelFillable\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leoyi\\LaravelFillable\\LaravelFillableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
