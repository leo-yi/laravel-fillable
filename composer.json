{
    "name": "leo-yi/laravel-fillable",
    "description": "a package show table columns and comment",
    "keywords": [
        "leo-yi",
        "laravel",
        "laravel-fillable"
    ],
    "homepage": "https://github.com/leo-yi/laravel-fillable",
    "license": "MIT",
    "authors": [
        {
            "name": "leo",
            "email": "uyihoo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=6.2",
        "doctrine/dbal": "^3.2",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Leoyi\\LaravelFillable\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leoyi\\LaravelFillable\\LaravelFillableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
